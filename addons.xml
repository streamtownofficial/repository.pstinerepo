<addons>
<addon id="repository.pstinerepo" name="P-stine Repo" version="0.0.1" provider-name="Stream Town">
	<extension point="xbmc.addon.repository" name="P-stine Repo">
        <info compressed="false">https://github.com/streamtownofficial/repository.pstinerepo/raw/master/addons.xml</info>
        <checksum>https://github.com/streamtownofficial/repository.pstinerepo/raw/master/addons.xml.md5</checksum>
        <datadir zip="true">https://github.com/streamtownofficial/repository.pstinerepo/raw/master/zips</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Official Repo for P-stine</summary>
        <description>install add-ons made by the P-stine Team. also will have different add-ons by different developers..</description>
        <platform>all</platform>
    </extension>
</addon>
<addon id="plugin.video.kobein"
       name="[COLOR purple]KobeIN[/COLOR]"
       version="1.0.0"
       provider-name="P-Stine">
  <requires>
    <import addon="xbmc.python" version="2.24.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <language>sq</language>
    <summary lang="en"></summary>
    <summary lang="sq"> </summary>
    <description lang="en">  stream beIN sports from around the world. All streams are not hosted or affiliated by us. </description>
    <description lang="nb"> stream beIN sports from around the world. All streams are not hosted or affiliated by us.</description>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum></forum>
    <source></source>
  </extension>
</addon>
<addon id="plugin.video.subzero" name="[COLOR blue]SubZero MMA[/COLOR]" version="1.0.0" provider-name="Razer">
<requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.t0mm0.common" version="2.0.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="script.module.urlresolver" />
    <import addon="script.module.simplejson" /> 
    <import addon="script.common.plugin.cache" version="2.5.2"/>
    <import addon="script.module.metahandler" version="2.5.1"/>
    <import addon="plugin.video.youtube" />
</requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">MMA Brought To You By Razer</summary>
    <description lang="en">This is a new project, content added on a daily basis!</description>
    <disclaimer lang="en">Credit: Merlin Project And Livestreams Pro</disclaimer>
    <platform>all</platform>
  </extension>
</addon>
</addons>